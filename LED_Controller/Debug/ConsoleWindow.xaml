<Window x:Class="LED_Controller.Debug.ConsoleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LED_Controller.Debug"
        mc:Ignorable="d"
        Title="ConsoleWindow" Height="450" Width="800"
        Closed="ConsoleWindow_OnClosed">
    <Grid>
        <ScrollViewer Name="ScrollViewer" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
            <TextBlock Name="Output"></TextBlock>
        </ScrollViewer>
    </Grid>
</Window>
